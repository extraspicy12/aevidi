                  @media screen and (max-width: 757px) {
                    width: 50%;
                  }
                  a {
                    color: $gray;
                    font-weight: 400;
                  }
                  a.active {
                    color: $header;
                    font-weight: 600;
                  }
                  a:hover {
                    color: $header;
                  }
                }
              }
            }
          }
          #sort-button-holder {
            #clear-holder {
              text-align: right;
            }
            #sort-holder {
              text-align: left;
            }
          }
