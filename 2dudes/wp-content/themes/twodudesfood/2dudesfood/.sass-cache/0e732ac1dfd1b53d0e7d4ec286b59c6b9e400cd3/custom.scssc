@import url(http://fonts.googleapis.com/css?family=Open+Sans);
@import url(http://fonts.googleapis.com/css?family=Josefin+Slab);


h1,h2,h3,h4,h5, ul li a, input, textarea, a.button {
  font-family: "Sanchez";
  weight: 400;
}
body,li,ul,p {
  font-family: "Open Sans";
}

h1.site-title {
 letter-spacing: 3px;
}

$header: #2e8445;
$sidecontainer: #92b999;
$accent: #47a761;
$offwhite: #DAFFE9;
$gray: #AEAEAE;

.divider.white {
  background-color: white;
  display: block;
  padding: 5px;
}
@media screen and (max-width: 757px) {
  .divider.white {
    display: none;
  }
}
#loader {
  display: none;
  text-align: center;
  margin-top: 33%;
  top: -128px;
}

.container {
  time.updated:before {
    content: " (Updated on ";
  }
  time.updated:after {
    content: ")";
  }
  a {
    transition: all .125s ease-in-out;
  }
  a:hover {
    transition: all .125s ease-in-out;
    color: $gray;
  }
  .button-primary, #submit {
    @include button-base;
    @include button-size;
    @include button-style;
    @include single-transition;
    @include inset-shadow(true);
  }
  footer.entry-meta {
    margin-top: 25px;
    text-align: center;
  }
  background-color: $offwhite;
  > header {
    background-color: white;
    h1 {
      font-weight: 400;
    }
    background-color: $header;
    .row {
      padding: 0 15px;
    }
    .logo {
      padding: 10px 0;
    }
    .top-nav {
      position: relative;
      float: right;
      ul {
        position: relative;
        float: right;
        margin: 0;
        padding: 0;
        li {
          list-style: none;
          font-size: .85em;
          margin: 0;
          color: white;
          text-transform: uppercase;
          letter-spacing: 1px;
          font-weight: 500;
          letter-spacing: 1.5px;
          position: relative;
          float: left;
          a {
            color: white;
            display: block;
            background-color: $accent;
            transition: all .2s ease-in-out;
            padding: 7.5px;
          }
          a:hover {
            transition: all .2s ease-in-out;
            color: $accent;
            background-color: $sidecontainer;
          }
        }
        li:nth-child(1) {
          a {
            border-bottom-left-radius: 5px;
          }
        }
        li:nth-child(3) {
          a {
            border-bottom-right-radius: 5px;
          }
        }
      }
    }
    .bottom {
      background-color: $accent;
      padding: 10px 0 0 0;
      .search-toggle {
        display:none;
        padding: 0 10px;
        form {
          margin-bottom: 0 !important;
        }
        .search-bar {
          padding: 0 0 10px 0;
          input, a {
            margin: 0;
          }
        }
      }
    }
    @media screen and (max-width:757px) {
    .bottom {
      background-color: $header;
      border-bottom: 5px solid $accent;
        .search-toggle {
          display: block;
        }
      }
    }
    .toggleMenu {
      display: none;
      background: $sidecontainer;
      padding: 15px 15px;
      color: white;
      width: 100%;
      position: relative;
    }
    .toggleMenu:after {
      content:"";
      background: url('../stylesheets/nav-icon.png') no-repeat;
      width: 20px;
      height: 20px;
      display: inline-block;
      position: absolute;
      right: 15px;
      top: 13px;
    }
    @media screen and (max-width:757px) {
      .toggleMenu {
        display: block;
      }
    }
    nav {
      .menu > ul:first-of-type {
        list-style: none;
        *zoom: 1;
        background: $header;
        position: relative;
        float: left;
        width: 90%;
        margin-bottom: 0;
        display: block;
        ul {
          list-style: none;
          width: 9em;
          margin: 0;
          padding: 0;
        }
        a {
          padding: 10px 15px;
          color: white;
          *zoom: 1;
          display: block;
        }
        > li.current_page_item {
          a {
            background-color: $accent;
          }
        }
        > li {
          float: left;
          z-index: 200;
          display: block;
        }
        > li.hover > ul {
          left: 0;
        }
        li {
          position: relative;
          li.hover ul {
            left: 100%;
            top: 0;
          }
          a {
            background-color: $header;
          }
          @media screen and (min-width: 758px) {
            a:hover {
              background-color: $accent;
            }
          }
          ul {
            position: absolute;
            left: -9999px;
            z-index: 100;
            width: 100%;
            li {
              a {
                background-color: $header;
                position: relative;
                z-index: 100;
              }
              li {
                a {
                  background-color: $sidecontainer;
                  z-index: 200;
                }
              }
            }
          }
        }
      }
      @media screen and (max-width:757px) {
        .menu > ul:first-of-type {
          width: 100%;
          display: none;
        }
        .menu > ul:first-of-type > li {
          float: none;
          width: 100%;
        }
        .menu > ul:first-of-type ul {
          display: block;
          width: 100%;
        }
        .menu > ul:first-of-type > li.hover > ul, .menu > ul:first-of-type li li.hover ul {
          position: static;
        }
        .menu > ul:first-of-type > li > ul > li a {
          background-color: $accent;
        }
        .menu > ul:first-of-type > li > ul > li > ul > li a {
          background-color: $sidecontainer !important;
        }
      }
      .menu > ul:first-of-type:before, .menu > ul:first-of-type:after {
        content: " ";
        display: table;
      }
      .menu > ul:first-of-type:after {
        clear: both;
      }
    }
    .search-ul {
      list-style: none;
      position: relative;
      float: left;
      width: 10%;
      height: 46px;
      margin: 0;
      padding: 0;
      overflow: none;
      display: block;
      .search-toggle-button {
        position: relative;
        float: right;
        display: block;
        height: 100%;
        width: 100%;
        padding: 5px;
        background: $accent url(../stylesheets/search.png) no-repeat center center;
      }
    }
    @media screen and (max-width:757px) {
      .search-ul {
        display: none;
      }
    }
  }
  .page-container {
    padding: 0 0 15px 15px;
    @media screen and (max-width:757px) {
      padding: 0;
    }
    background-color: white;
    .body {
      #nav-below {
      margin-top: 50px;
        div {
          .nav-previous {
            text-align: left;
          }
