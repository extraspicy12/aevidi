      #comments {
        h2 {
          font-size: 2.25em;
          font-weight: 400;
        }
        #reply-title { display: none; }
        #respond {
          #commentform {
            .logged-in-as { display: none; }
            .comment-form-comment {
              label { display: none; }
              textarea#comment {
                background-color: #EEEEEE;
                resize: none;
                height: 150px;
              }
            }
            .form-allowed-tags { display: none; }
            .form-submit {
              text-align: right;
              @media screen and (max-width:757px) {
                text-align: center;
              }
            }
          }
        }
        ol.comment-list {
          li article {
            padding-bottom: 20px;
            .comment-content p {
              padding-bottom: 0;
            }
          }
          li.comment, ul.children {
            list-style: none;
          }
          ul.children {
            padding-left: 5%;
            @media screen and (max-width:757px) {
              padding-left: 0;
            }
          }
          > li.comment:nth-of-type(odd) {
            background-color: $offwhite; /* #e3e3e3 */
/*
            border-top-left-radius: 5px;
            border-top-right-radius: 5px;
*/
          }
          li.comment {
            div.comment-avatar {
              img {
                min-width: 50px;
                width: 100%;
                height: auto;
              }
              padding-right: 10px;
            }
            footer.comment-meta {
              span, time {
                display: inline-block;
                height: 100%;
              }
              background-color: #EEEEEE;
              padding: 5px;
/*               border-radius: 5px; */
              line-height: 36px;
              div.comment-author {
                  position: relative;
                  float: left;
                  font-size: 24px;
                  span.author-name {
                    color: $header;
                  }
              }
              div.comment-reply {
                position: relative;
                float: left;
                font-size: 16px;
                color: $gray;
                padding-left: 27.5px;
                .arrow-right {
                  	width: 0;
                  	height: 0;
                  	border-top: 5px solid transparent;
                  	border-bottom: 5px solid transparent;
                  	border-left: 10px solid $gray;
                    position: absolute;
                    left: 10px;
                    top: 50%;  /* added */
                    margin-top: -5px;
                 }
              }
              div.comment-metadata {
                text-align: right;
                time {
                  color: $gray;
                }
              }
            }
          }
        }
      }
