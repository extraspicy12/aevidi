            }
            .bad {
              border-bottom-color: none;
              color: #BD9751;
              background-color: transparent;
              font-weight: 600;
            }
            .poor {
              border-bottom-color: none;
              color: #BD5B51;
              background-color: transparent;
              font-weight: 600;
            }
          }
          .rating {
            margin: 0 0 20px 0;
            h1 {
              font-size: 5em;
              line-height: 100px;
              font-weight: 600;
              font-family: "Sanchez";
              padding: 20px 10px;
              border-bottom-width: 5px;
              border-bottom-style: solid;
              text-align: center;
            }
          }
          blockquote {
            font-size: 1.5em;
            font-weight: 500;
            font-family: "Sanchez";
            padding: 5px 10px;
            color: $gray;
            quotes: "\201C""\201D""\2018""\2019";
            text-align: center;
            border: none;
            margin: 0 15px;
            .great {
              border-bottom-color: none;
              color: $header;
              background-color: transparent;
            }
            .good {
              border-bottom-color: none;
              color: #85AF4B;
              background-color: transparent;
            }
            .average {
              border-bottom-color: none;
              color: #B5BA50;
              background-color: transparent;
            }
            .bad {
              border-bottom-color: none;
              color: #BD9751;
              background-color: transparent;
            }
            .poor {
              border-bottom-color: none;
              color: #BD5B51;
              background-color: transparent;
