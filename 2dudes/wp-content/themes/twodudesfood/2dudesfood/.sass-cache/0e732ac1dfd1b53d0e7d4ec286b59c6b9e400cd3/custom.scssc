:@template"N8@import url(http://fonts.googleapis.com/css?family=Sanchez);
@import url(http://fonts.googleapis.com/css?family=Open+Sans);
@import url(http://fonts.googleapis.com/css?family=Josefin+Slab);


h1,h2,h3,h4,h5, ul li a, input, textarea, a.button {
  font-family: "Sanchez";
  weight: 400;
}
body,li,ul,p {
  font-family: "Open Sans";
}

$header: #2e8445;
$sidecontainer: #92b999;
$accent: #47a761;
$offwhite: #DAFFE9;

header .divider {
  display: none;
}
.divider.white {
  background-color: white;
  display: block;
  padding: 5px;
}
@media screen and (max-width: 757px) {
  header .divider {
    display: block;
    padding: 5px;
    background-color: $header;
  }
}

.container {
  a {
    transition: all .125s ease-in-out;
  }
  a:hover {
    transition: all .125s ease-in-out;
    color: #CCCCCC;
  }
  .button-primary, #submit {
    @include button-base;
    @include button-size;
    @include button-style;
    @include single-transition;
    @include inset-shadow(true);
  }
  footer.entry-meta {
    > span {
      margin-right: 10px;
    }
  }
  background-color: $offwhite;
  > header {
    background-color: white;
    h1 {
      font-weight: 400;
    }
    background-color: $header;
    .row {
      padding: 0 15px;
    }
    .logo {
      padding: 10px 0;
    }
    .top-nav {
      position: relative;
      float: right;
      ul {
        position: relative;
        float: right;
        margin: 0;
        padding: 0;
        li {
          list-style: none;
          font-size: .85em;
          margin: 0;
          color: white;
          text-transform: uppercase;
          letter-spacing: 1px;
          font-weight: 500;
          letter-spacing: 1.5px;
          position: relative;
          float: left;
          a {
            color: white;
            display: block;
            background-color: $accent;
            transition: all .2s ease-in-out;
            padding: 7.5px;
          }
          a:hover {
            transition: all .2s ease-in-out;
            color: $accent;
            background-color: $sidecontainer;
          }
        }
        li:nth-child(1) {
          a {
            border-bottom-left-radius: 5px;
          }
        }
        li:nth-child(3) {
          a {
            border-bottom-right-radius: 5px;
          }
        }
      }
    }
    .bottom {
      background-color: $accent;
      padding: 10px 0 0 0;
      .search-toggle {
        display:none;
        padding: 0 10px;
        form {
          margin-bottom: 0 !important;
        }
        .search-bar {
          padding: 0 0 10px 0;
          input, a {
            margin: 0;
          }
        }
      }
    }
    @media screen and (max-width:757px) {
    .bottom {
      background-color: $header;
      border-bottom: 5px solid $accent;
        .search-toggle {
          display: block;
        }
      }
    }
    .toggleMenu {
      display: none;
      background: $sidecontainer;
      padding: 15px 15px;
      color: white;
      width: 100%;
      position: relative;
    }
    .toggleMenu:after {
      content:"";
      background: url('../stylesheets/nav-icon.png') no-repeat;
      width: 20px;
      height: 20px;
      display: inline-block;
      position: absolute;
      right: 15px;
      top: 13px;
    }
    @media screen and (max-width:757px) {
      .toggleMenu {
        display: block;
      }
    }
    nav {
      .menu > ul:first-of-type {
        list-style: none;
        *zoom: 1;
        background: $header;
        position: relative;
        float: left;
        width: 90%;
        margin-bottom: 0;
        display: block;
        ul {
          list-style: none;
          width: 9em;
          margin: 0;
          padding: 0;
        }
        a {
          padding: 10px 15px;
          color: white;
          *zoom: 1;
          display: block;
        }
        > li.current_page_item {
          a {
            background-color: $accent;
          }
        }
        > li {
          float: left;
          z-index: 200;
          display: block;
        }
        > li.hover > ul {
          left: 0;
        }
        li {
          position: relative;
          li.hover ul {
            left: 100%;
            top: 0;
          }
          a {
            background-color: $header;
          }
          @media screen and (min-width: 758px) {
            a:hover {
              background-color: $accent;
            }
          }
          ul {
            position: absolute;
            left: -9999px;
            z-index: 100;
            width: 100%;
            li {
              a {
                background-color: $header;
                position: relative;
                z-index: 100;
              }
              li {
                a {
                  background-color: $sidecontainer;
                  z-index: 200;
                }
              }
            }
          }
        }
      }
      @media screen and (max-width:757px) {
        .menu > ul:first-of-type {
          width: 100%;
          display: none;
        }
        .menu > ul:first-of-type > li {
          float: none;
          width: 100%;
        }
        .menu > ul:first-of-type ul {
          display: block;
          width: 100%;
        }
        .menu > ul:first-of-type > li.hover > ul, .menu > ul:first-of-type li li.hover ul {
          position: static;
        }
        .menu > ul:first-of-type > li > ul > li a {
          background-color: $accent;
        }
        .menu > ul:first-of-type > li > ul > li > ul > li a {
          background-color: $sidecontainer !important;
        }
      }
      .menu > ul:first-of-type:before, .menu > ul:first-of-type:after {
        content: " ";
        display: table;
      }
      .menu > ul:first-of-type:after {
        clear: both;
      }
    }
    .search-ul {
      list-style: none;
      position: relative;
      float: left;
      width: 10%;
      height: 46px;
      margin: 0;
      padding: 0;
      overflow: none;
      display: block;
      .search-toggle-button {
        position: relative;
        float: right;
        display: block;
        height: 100%;
        width: 100%;
        padding: 5px;
        background: $accent url(../stylesheets/search.png) no-repeat center center;
      }
    }
    @media screen and (max-width:757px) {
      .search-ul {
        display: none;
      }
    }
  }

  .page-container {
    padding: 0 15px 15px 15px;
    background-color: white;
    .body {
      #nav-below {
        div {
          .nav-previous {
            text-align: left;
          }
          .nav-next {
            text-align: right;
          }
        }
      }
      .slider {
        ul {
          li {
            img {
              min-width: 100%;
              height: auto;
            }
          }
        }
        .orbit-caption {
          h3 {
            color: white;
            margin-bottom: 0;
            font-weight: 400;
          }
          h5 {
            margin-top: 0;
            color: #AEAEAE;
            font-size: .75em;
            font-weight: 400;
            text-transform: uppercase;
          }
        }
      }
      article {
        margin-bottom: 30px;
        header {
          h1 {
            font-weight: 500;
            font-size: 3em;
            margin-bottom: 12px;
          }
          time.updated:before {
            content: " (Updated on ";
          }
          time.updated:after {
            content: ")";
          }
        }
        h2 {
          margin-bottom: 0;
          font-weight: 400;
          font-size: 2.25em;
        }
        h5 {
          margin-top: 0;
          color: #AEAEAE;
          font-size: .9em;
          font-weight: 400;
          text-transform: uppercase;
        }
        p {}
        .divider {
          background-color: $accent;
          margin-bottom: 10px;
          height: 3px;
          width: 100%;
        }
      }
      #comments {
        h2 {
          font-size: 2.25em;
          font-weight: 400;
        }
        #reply-title { display: none; }
        #respond {
          #commentform {
            .logged-in-as { display: none; }
            .comment-form-comment {
              label { display: none; }
              textarea#comment {
                background-color: #EEEEEE;
                resize: none;
                height: 150px;
              }
            }
            .form-allowed-tags { display: none; }
            .form-submit {
              text-align: right;
            }
          }
        }
        ol.comment-list {
          li.comment, ul.children {
            list-style: none;
          }
          ul.children {
            margin-left: 5%;
          }
          li.comment {
            div.comment-avatar {
              img {
                max-width: 72px;
                width: 100%;
                height: auto;
              }
              padding-right: 10px;
            }
            footer.comment-meta {
              background-color: #EEEEEE;
              padding: 5px;
              div.comment-author {
                color: $header;
                position: relative;
                float: left;
                font-size: 1em;
              }
              div.comment-metadata {
                position: relative;
                float: right;
                text-align: right;
                time {
                  color: #888888;
                }
              }
            }
          }
        }
      }
    }
    .sidebar {
      .collapse {
         padding: 5px 15px 0 15px;
      }
      .social-media {
        padding-top: 20px;
        padding-bottom: 20px;
        text-align: center;
        a {
          transition: all .2s ease-in-out;
        }
        a:hover {
          transition: all .2s ease-in-out;
        	opacity: .7;
        	filter:alpha(opacity=70);
        }
      }
      aside {
        margin: 0 0 15px 0;
        background-color: $accent;
        border-top: 10px solid $header;
        padding: 10px;
        border-radius: 5px;
        .avatar {
          height: 60px;
          width: 60px;
        }
        ul {
          padding-left: 15px;
          li {
          color: $offwhite;
          list-style: circle;
          }
        }
        a {
          color: $offwhite;
        }
        a:hover{
          color: #CCCCCC;
        }
        h1 {
          color: $offwhite;
          font-size: 1.75em;
          font-weight: 500;
          text-align: center;
        }
      }
      .widget_wp_sidebarlogin {
        background-color: $accent;
        border-top: 10px solid $header;
        padding: 10px;
        ul {
          padding-left: 0;
          text-align: left;
          font-weight: 500;
          li {
            list-style: none;
          }
        }
        .button-primary {
          margin-bottom: 0;
          display: block;
          width: 100%;
        }
        h1 {
          color: $offwhite;
          font-size: 1.75em;
          text-align: center;
        }
        p {
          label {
            color: $offwhite;
            font-weight: 200;
            font-size: .875em;
            text-transform: uppercase;
          }
        }
      }
      @media screen and (max-width: 850px) {
        .widget.widget_wp_sidebarlogin {
          display: block;
          text-align: center;
        }
        .avatar_container {
          width: 100%;
          text-align: center;
        }
        ul {
          text-align: center !important;
        }
      }
      @media screen and (max-width: 757px) {
        > aside {
           display: none;
        }
      }
      .user-area-logged-out {
        .button-primary { @include button;}
        .section-container {
          border: none;
          section {
            border: none;
            p.title {
              border-top: 10px solid $header;
              border-left: none;
              border-right: none;
              border-bottom: none;
              background-color: $accent;
              a {
                color: $offwhite;
                font-weight: 350;
                font-size: 1.25em;
                padding: 7.5px 20px;
              }
            }
            div.content {
              padding: 7.5px 20px;
              background-color: $accent;
              border: none;
              input {
                margin-top: 10px;
                margin-bottom: 3px;
                background-color: $offwhite;
              }
              label {
                color: $offwhite;
                font-weight: 200;
              }
              .button {
                margin: 10px 0 0 0;
              }
            }
          }
        }
      }
    }
  }
  > footer {
    background-color: $accent;
    border-top: 10px solid $header;
    #footer {
      padding: 15px;
      .aevidi_widget_recent_comments {
        > ul#recentcomments {
          > li.recentcomments {
            list-style: none;
            padding: 10px;
            background-color: $header;
            margin: 10px 0;
            border-radius: 5px;
            width: 100%;
            div.user-box {
              position: relative;
              padding: 0 7.5px 7.5px 0;
              text-align: center;
              border-radius: 5px;
              h5 {
                color: $offwhite;
                font-weight: 600;
                font-size: 1em;
              }
            }
          }
        }
      }
    }
    h1 {
      text-align: center;
      font-size: 1.5em;
      font-weight: 500;
    }
    ul {
      li {
        a {
          color: $offwhite;
        }
      }
    }
    .copyright {
      background-color: $header;
      padding: 15px 0px;
      text-align: center;
      font-size: .9em;
      font-family: 'Sanchez';
      a {
        color: $offwhite;
      }
      a:hover {
        color: #CCCCCC;
      }
    }
  }
}

/* Centering Classes */

/* This parent can be any width and height */
.block {
  text-align: center;
}

/* The ghost, nudged to maintain perfect centering */
.block:before {
  content: '';
  display: inline-block;
  height: 100%;
  vertical-align: middle;
  margin-right: -0.25em; /* Adjusts for spacing */
}

/* The element to be centered, can
   also be of any width and height */
.centered {
  display: inline-block;
  vertical-align: middle;
}
.no-pad {
  padding: 0 !important;
}
