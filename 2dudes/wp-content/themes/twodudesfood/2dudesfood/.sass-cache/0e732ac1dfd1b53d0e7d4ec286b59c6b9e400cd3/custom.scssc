    .sidebar {
      .collapse {
         padding: 5px 15px 0 15px;
      }
      .social-media {
        padding-top: 20px;
        padding-bottom: 20px;
        text-align: center;
        a {
          transition: all .2s ease-in-out;
        }
        a:hover {
          transition: all .2s ease-in-out;
        	opacity: .7;
        	filter:alpha(opacity=70);
        }
